source":"src/EnemySprite.js","original":{"line":60,"column":18}},{"generated":{"line":104,"column":18},"source":"src/EnemySprite.js","original":{"line":60,"column":12}},{"name":"curName","generated":{"line":104,"column":21},"source":"src/EnemySprite.js","original":{"line":60,"column":25}},{"generated":{"line":104,"column":28},"source":"src/EnemySprite.js","original":{"line":60,"column":32}},{"name":"name","generated":{"line":104,"column":29},"source":"src/EnemySprite.js","original":{"line":60,"column":33}},{"generated":{"line":104,"column":33},"source":"src/EnemySprite.js","original":{"line":60,"column":12}},{"generated":{"line":105,"column":0},"source":"src/EnemySprite.js","original":{"line":61,"column":9}},{"generated":{"line":106,"column":0},"source":"src/EnemySprite.js","original":{"line":62,"column":5}},{"name":"Phaser","generated":{"line":110,"column":2},"source":"src/EnemySprite.js","original":{"line":8,"column":33}},{"generated":{"line":110,"column":8},"source":"src/EnemySprite.js","original":{"line":8,"column":39}},{"name":"Physics","generated":{"line":110,"column":9},"source":"src/EnemySprite.js","original":{"line":8,"column":40}},{"generated":{"line":110,"column":16},"source":"src/EnemySprite.js","original":{"line":8,"column":33}},{"name":"Arcade","generated":{"line":110,"column":17},"source":"src/EnemySprite.js","original":{"line":8,"column":48}},{"generated":{"line":110,"column":23},"source":"src/EnemySprite.js","original":{"line":8,"column":33}},{"name":"Sprite","generated":{"line":110,"column":24},"source":"src/EnemySprite.js","original":{"line":8,"column":55}}],"sources":{"src/EnemySprite.js":"/* File Name: EnemySprite.js\r\n * Author: Mathew Boland\r\n * Last Updated: November 8, 2019\r\n * Description: A class to create and hold the value of a CharacterSprite object\r\n * with arcade physics.\r\n * Citation: Code adapted from: https://github.com/jestarray/gate/tree/yt, jestarray\r\n*/\r\nexport class EnemySprite extends Phaser.Physics.Arcade.Sprite {\r\n    \r\n    constructor(scene, x, y, texture, frame, name, hp) {\r\n        super(scene, x, y, texture, frame);\r\n        scene.sys.updateList.add(this);\r\n        scene.sys.displayList.add(this);\r\n        this.setScale(2);        \r\n        scene.physics.world.enableBody(this);\r\n        this.setImmovable(true);\r\n        this.hp = hp;\r\n        this.name = name;\r\n        this.startX = x;\r\n        this.startY = y;\r\n        this.collideName = '';\r\n    }\r\n\r\n    enemyCollide(player, enemy){\r\n        let curName;\r\n        //Save whichever is in use to a temp variable to test conditions with\r\n        if(player.name != ''){\r\n            curName = player;\r\n        }else{\r\n            curName = enemy;\r\n        }\r\n        //Based on the name from the collision decide what to do\r\n        switch(curName.name){\r\n            //For nerds just switch their current direction, slice their name so that it keeps the same variation number\r\n            case \"nerd1up\":\r\n            case \"nerd2up\":\r\n                curName.setVelocityY(-90);\r\n                curName.name = curName.name.slice(0, 5)+\"down\";\r\n            break;\r\n            case \"nerd1down\":\r\n            case \"nerd2down\":  \r\n                curName.setVelocityY(90);\r\n                curName.name = curName.name.slice(0, 5)+\"up\";\r\n            break;\r\n            case \"nerd1right\":\r\n            case \"nerd2right\":  \r\n                curName.setVelocityX(90);\r\n                curName.name = curName.name.slice(0, 5)+\"left\"; \r\n            break;\r\n            case \"nerd1left\":\r\n            case \"nerd2left\":  \r\n                curName.setVelocityX(-90);\r\n                curName.name = curName.name.slice(0, 5)+\"right\";\r\n            break;\r\n        }\r\n        //Now store the temp variable back into the game object\r\n        if(player.name != ''){\r\n            player.name = curName.name;\r\n        }else{\r\n            enemy.name = curName.name;\r\n        }  \r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"d8f4d6129e6238bb78003ca7e546523f","cacheData":{"env":{}}}